pipeline {
	agent none
	options { skipDefaultCheckout(false) }

	stages {

		stage('Docker build') {
      agent any
			steps {
				sh 'docker build -t frontend:latest /var/jenkins_home/workspace/BalTo/frontend'
				sh 'docker build -t backend:latest /var/jenkins_home/workspace/BalTo/backend'
			}
		}
	}
}